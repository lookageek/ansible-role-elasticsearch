---
# {{ ansible_managed }}
# Remember, leave a key empty if there is no value.  None will be a string,
# not a Python "NoneType"

client:
  hosts:
    - {{ elasticsearch_network_host }}
  port: {{ elasticsearch_http_port }}
  url_prefix:
  use_ssl:
  certificate:
  client_cert:
  client_key:
  ssl_no_validate:
  http_auth:
  timeout: 30
  master_only:

logging:
  loglevel:
  logfile:
  logformat:
  blacklist:
